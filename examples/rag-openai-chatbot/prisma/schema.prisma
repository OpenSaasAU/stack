generator client {
  provider = "prisma-client"
  output   = "../.opensaas/prisma-client"
}

datasource db {
  provider = "postgresql"
}

model KnowledgeBase {
  id               String   @id @default(cuid())
  title            String
  content          String
  category         String
  published        Boolean  @default(true)
  contentEmbedding Json?
  createdAt        DateTime @default(now())
  updatedAt        DateTime @updatedAt
}
