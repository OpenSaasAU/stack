  },
  "dependencies": {
    "@opensaas/stack-storage": "workspace:*",
    "@vercel/blob": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "@vitest/coverage-v8": "^4.0.5",
    "typescript": "^5.7.3",
    "vitest": "^4.0.5"
