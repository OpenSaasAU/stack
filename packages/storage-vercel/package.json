{
  "name": "@opensaas/stack-storage-vercel",
  "version": "0.1.2",
  "description": "Vercel Blob storage provider for OpenSaas Stack file uploads",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
