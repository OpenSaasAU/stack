    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.709.0",
    "@aws-sdk/s3-request-presigner": "^3.709.0",
    "@opensaas/stack-storage": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "@vitest/coverage-v8": "^4.0.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.0"
  },
  "peerDependencies": {
    "@opensaas/stack-storage": "^0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenSaasAU/stack.git",
    "directory": "packages/storage-s3"
  },
  "homepage": "https://stack.opensaas.au",
  "bugs": {
    "url": "https://github.com/OpenSaasAU/stack/issues"
