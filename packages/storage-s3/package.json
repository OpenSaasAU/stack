{
  "name": "@opensaas/stack-storage-s3",
  "version": "0.2.0",
  "description": "AWS S3 storage provider for OpenSaas Stack file uploads",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
